{
  "name": "unframe",
  "version": "1.0.0",
  "description": "Home assigment",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "queue": "node index2.js",
    "queue_dev": "nodemon index2.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "db:migrate": "sequelize-cli db:migrate",
    "migrate:generate": "sequelize-cli migration:generate --name",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:seed": "sequelize-cli db:seed:all",
    "db:reset": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate",
    "setup:db": "node scripts/setup-db.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run queue_dev\""
  },
  "keywords": [],
  "author": "Shachar Bogin",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.0",
    "fs": "^0.0.1-security",
    "googleapis": "^144.0.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.2",
    "lodash": "^4.17.21",
    "mongodb": "^6.12.0",
    "mysql2": "^3.11.5",
    "node-cache": "^5.1.2",
    "openai": "^4.76.3",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "redis": "^4.7.0",
    "sanitize-html": "^2.14.0",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "concurrently": "^8.0.0",
    "nodemon": "^3.0.0"
  }
}
